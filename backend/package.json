    "aws-sdk-fluent-builder": "0.3.1",
    "read-gedcom": "0.3.2",
    "typescript-inject": "0.0.8"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.109",
    "@types/jest": "29.4.0",
    "@types/node": "16.11.7",
    "@types/uuid": "9.0.0",
    "jest": "29.4.1",
    "serverless-plugin-typescript": "2.1.4",
    "serverless-plugin-thundra": "2.6.2",
    "serverless-plugin-umzug": "1.3.0",
    "ts-jest": "29.0.5",
