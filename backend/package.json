  "main": "handler.js",
  "scripts": {
    "test": "jest --coverage",
    "test-watch": "jest --watch",
    "deploy-dev": "sed -i 's/THUNDRAAPIKEY/'\"$THUNDRA_API_KEY\"'/g' serverless.yml && serverless deploy --stage=dev",
